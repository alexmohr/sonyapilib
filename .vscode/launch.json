    "version": "0.2.0",
    "configurations": [
     {
         "name": "Python: Current File",
         "type": "python",
         "request": "launch",
         "stopOnEntry": false,
         "pythonPath": "${config:python.pythonPath}",
         "program": "${file}",
         "cwd": "${workspaceFolder}",
         "env": {},
         "envFile": "${workspaceFolder}/.env",
         "debugOptions": [
             "RedirectOutput"
         ]
     }
    ]
}